alias gith='cd /home/ballakottan/Documents/github'
alias upp='sudo apt-get update && sudo apt-get upgrade'
alias Xstart='sudo /opt/lampp/lampp start'
alias Xstop='sudo /opt/lampp/lampp stop'
alias Xsql='sudo /opt/lampp/bin/mysql -uroot'
alias ArmSim='mono /home/ballakottan/.dotnet/ARMSim-191-MAC/ARMSim.exe'
alias UML='echo java -jar plantuml.jar textName'
alias ALIAS='echo -e "AliasList: 
        gith: Takes You To the github Directory
        upp: Will run Update && Upgrade
        Xstart: Start Xampp Server
        Xstop: Stops Xampp Server
        Xsql: Enter MariaDB
        ArmSim: Run ARMsim
        UML: Echo how to create UML png
"'
